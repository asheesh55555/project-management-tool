<%= form_for :assign_project_member, url: assigned_member_project_team_path(@team) do |f| %>

   <div class="form-group">
    <%= f.label :user %>
    <%= f.select("user_id", @users.collect {|u| [ u.email, u.id ] }, { include_blank: true },class: "form-control", :required => true)%>
  </div>
  

  <div class="form-group">
    <%= f.label :project %>
    <%= f.select("project_id", @projects.collect {|p| [ p.name, p.id ] }, { include_blank: true },class: "form-control", :required => true) %>
  </div>


  <p>
    <%= f.submit 'Assign', class: "btn btn-primary"%>
  </p>
<% end %>



