<%#= render "devise/shared/links" %>
<section id="contact">
    <div class="container">
        <h2 class="text-center text-uppercase text-secondary mb-0">Login In</h2>
        <hr class="star-dark mb-5">
        <div class="row">
            <div class="col-lg-8 mx-auto">
                <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                    <div class="control-group">
                        <div class="form-group floating-label-form-group controls mb-0 pb-2">
                            <label>Email</label>
                            <%= f.email_field :email, autofocus: true, :class =>"form-control", :id=> "name", placeholder: "Email ",  required: "required",  "data-validation-required-message" => "Please enter your name."%>
                                <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="control-group">
                        <div class="form-group floating-label-form-group controls mb-0 pb-2">
                            <label>Password</label>
                            <%= f.password_field :password, autocomplete: "off", placeholder: 'Enter your Password' %>
                                <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div id="success"></div>
                    <div class="form-group">
                      
                        <%=f.submit "Login"%>
                    </div>
                    <% end %>
            </div>
        </div>
    </div>
</section>